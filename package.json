    "@vitest/coverage-v8": "^1.6.0",
    "husky": "^9.0.11",
    "tsup": "^8.3.6",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "turbo": "^2.4.0"
