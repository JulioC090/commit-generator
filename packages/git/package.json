{
  "name": "@commit-generator/git",
  "version": "0.0.0",
  "description": "Git integration utilities",
  "author": "JulioC090",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JulioC090/commit-generator.git",
    "directory": "packages/git"
  },
  "bugs": {
    "url": "https://github.com/JulioC090/commit-generator/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup --format cjs,esm",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit -p ."
  },
  "devDependencies": {
    "@commit-generator/typescript-config": "0.1.0",
    "@commit-generator/eslint-config": "0.1.0",
    "@types/node": "^22.10.2"
